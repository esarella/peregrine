<div class="container trips-photos-container">
  <%#= link_to "Add photo", new_trip_photo_path %>
  

  <div class="col s8" style="background-color: steelblue">
    <%#= render 'trip_photos' %>
    <div id="photo-gallery">
      <ul class="photo-list">
        <% @photos.each do |photo| %>
          <li>
            <%= image_tag single_photo(photo) %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>

  <%# practice form %>

  <%= link_to 'Add new photos', new_trip_photo_path %>


</div>
